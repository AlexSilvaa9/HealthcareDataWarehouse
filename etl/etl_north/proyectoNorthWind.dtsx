                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  C.CustomerKey, EmployeeID AS EmployeeKey,

 T.TimeKey  AS OrderDateKey, T1.TimeKey AS DueDateKey, T2.TimeKey AS ShippedDateKey,

ShipVia AS ShipperKey, P.ProductID AS ProductKey,

SupplierID AS SupplierKey, O.OrderID AS OrderNo,

CONVERT(INT, ROW_NUMBER() OVER (PARTITION BY D.OrderID ORDER BY D.ProductID)) AS OrderLineNo,

D.UnitPrice, Quantity, Discount, 

CONVERT(float, D.UnitPrice * (1-Discount) * Quantity) AS SalesAmount,

CONVERT(float, O.Freight/COUNT(*) OVER (PARTITION BY D.OrderID)) AS Freight

FROM Northwnd.dbo.Orders O join Northwnd.dbo.[Order Details] D on (O.OrderID = D.OrderID) 
join Northwnd.dbo.Products P on (D.ProductID = P.ProductID) 
left outer join dbo.Customer C on (C.CustomerID = O.CustomerID) 
left outer join dbo.Time T on (T.Date = O.OrderDate) 
left outer join dbo.Time T1 on (T1.Date = O.RequiredDate) 
left outer join dbo.Time T2 on (T2.Date = O.ShippedDate)</property>
